{
  "name": "wayfarer",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start:dev": "DEBUG=app,app:* && nodemon server.js",
    "start": "NODE_ENV=prod && node server.js",
    "test": "mocha --exit --timeout 10000",
    "test-travis": "mocha --exit",
    "prettier": "prettier '**/*.+(js|json|jsx|css)'",
    "format": "npm run prettier -- --write",
    "lint": "eslint src --fix",
    "beautify": "npm run lint && npm run prettier -- --list-different",
    "validate": "npm run beautify && npm run test"
  },
  "author": "Ayobami Ayo-Salami",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.1.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "setprototypeof": "^1.2.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
